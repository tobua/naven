{
  "scripts": {
    "build": "papua build",
    "lint": "papua lint",
    "start": "papua start",
    "test": "papua test",
    "update": "papua update"
  },
  "prettier": "papua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/papua/configuration/eslint.cjs"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "./node_modules/papua/configuration/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": [
        "babel-jest",
        {
          "configFile": "./node_modules/papua/configuration/.babelrc"
        }
      ]
    }
  },
  "dependencies": {
    "papua": "^0.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "localDependencies": {
    "naven": ".."
  }
}
