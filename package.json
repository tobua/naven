{
  "name": "naven",
  "description": "React based CSS-in-JS styling framework.",
  "version": "0.0.0-development",
  "repository": "github:tobua/naven",
  "homepage": "https://naven-documentation.vercel.app",
  "license": "MIT",
  "author": "Matthias Giger",
  "scripts": {
    "build": "patch-package && padua build",
    "start": "padua watch",
    "test": "vitest run"
  },
  "padua": {
    "tsconfig": {
      "include": [
        "./declarations.d.ts"
      ]
    },
    "esbuild": {
      "external": [
        "naven"
      ]
    },
    "entry": "feature/*"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.10.0",
    "@codesandbox/sandpack-themes": "^2.0.21",
    "@popperjs/core": "^2.11.8",
    "@stitches/react": "^1.2.8",
    "@types/react": "^18.2.38",
    "body-scroll-lock": "^3.1.5",
    "fast-deep-equal": "^3.1.3",
    "laier": "^1.2.2",
    "memoize-one": "^6.0.0",
    "object-assign-deep": "^0.4.0",
    "react-datepicker": "^4.23.0",
    "react-popper": "^2.3.0",
    "react-select": "^5.8.0",
    "throttle-debounce": "^5.0.0",
    "wasser": "^2.4.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/object-assign-deep": "^0.4.3",
    "@types/react-datepicker": "^4.19.3",
    "@types/react-dom": "^18.2.17",
    "@types/throttle-debounce": "^5.0.2",
    "jest-environment-jsdom": "^29.7.0",
    "padua": "^2.0.7",
    "patch-package": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./Code": {
      "types": "./dist/feature/Code.d.ts",
      "default": "./dist/feature/Code.js"
    },
    "./Date": {
      "types": "./dist/feature/Date.d.ts",
      "default": "./dist/feature/Date.js"
    },
    "./Dropdown": {
      "types": "./dist/feature/Dropdown.d.ts",
      "default": "./dist/feature/Dropdown.js"
    },
    "./Tooltip": {
      "types": "./dist/feature/Tooltip.d.ts",
      "default": "./dist/feature/Tooltip.js"
    }
  },
  "source": "./index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "ui",
    "ui-framework",
    "react",
    "css-in-js",
    "stitches"
  ],
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs",
    "rules": {
      "prefer-arrow-callback": 0,
      "react/function-component-definition": 0
    }
  },
  "publishConfig": {
    "provenance": true
  },
  "engines": {
    "node": ">= 18"
  }
}
