{
  "name": "naven",
  "description": "React based CSS-in-JS styling framework.",
  "version": "0.0.0-development",
  "repository": "github:tobua/naven",
  "homepage": "https://naven-documentation.vercel.app",
  "license": "MIT",
  "author": "Matthias Giger",
  "scripts": {
    "build": "padua build",
    "start": "padua watch",
    "test": "padua test"
  },
  "padua": {
    "tsconfig": {
      "compilerOptions": {
        "skipLibCheck": true,
        "target": "es6",
        "types": [
          "@types/jest"
        ],
        "lib": [
          "DOM",
          "ESNext"
        ]
      },
      "include": [
        "./declarations.d.ts"
      ]
    },
    "esbuild": {
      "external": [
        "naven"
      ]
    },
    "entry": "feature/*"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.6.7",
    "@codesandbox/sandpack-themes": "^2.0.21",
    "@popperjs/core": "^2.11.8",
    "@stitches/react": "^1.2.8",
    "@types/react": "^18.2.12",
    "body-scroll-lock": "^3.1.5",
    "fast-deep-equal": "^3.1.3",
    "laier": "^1.2.2",
    "memoize-one": "^6.0.0",
    "object-assign-deep": "^0.4.0",
    "react-datepicker": "^4.13.0",
    "react-popper": "^2.3.0",
    "react-select": "^5.7.3",
    "throttle-debounce": "^5.0.0",
    "wasser": "^2.3.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./Code": {
      "types": "./dist/feature/Code.d.ts",
      "default": "./dist/feature/Code.js"
    },
    "./Date": {
      "types": "./dist/feature/Date.d.ts",
      "default": "./dist/feature/Date.js"
    },
    "./Dropdown": {
      "types": "./dist/feature/Dropdown.d.ts",
      "default": "./dist/feature/Dropdown.js"
    },
    "./Tooltip": {
      "types": "./dist/feature/Tooltip.d.ts",
      "default": "./dist/feature/Tooltip.js"
    }
  },
  "source": "./index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "ui",
    "ui-framework",
    "react",
    "css-in-js",
    "stitches"
  ],
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/object-assign-deep": "^0.4.0",
    "@types/react-datepicker": "^4.11.2",
    "@types/react-dom": "^18.2.5",
    "@types/throttle-debounce": "^5.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "padua": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0"
  },
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs",
    "rules": {
      "prefer-arrow-callback": 0,
      "react/function-component-definition": 0
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "./tsconfig.json"
        }
      ],
      "^.+\\.jsx?$": [
        "babel-jest",
        {
          "configFile": "./node_modules/padua/configuration/.babelrc"
        }
      ]
    },
    "transformIgnorePatterns": []
  },
  "publishConfig": {
    "provenance": true
  },
  "engines": {
    "node": ">= 16"
  }
}
